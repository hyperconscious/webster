{
    "name": "webster",
    "version": "1.0.0",
    "description": "Node.js Rest Apis with Express, TypeOrm & MySQL",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "nodemon",
        "start:dist": "ts-node dist/index.js",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/config/orm.config.ts",
        "db:drop": "npm run typeorm:cli schema:drop",
        "migration:generate": "npm run typeorm:cli migration:generate -n",
        "db:migrate": "npm run typeorm:cli migration:run",
        "db:revert": "npm run typeorm:cli migration:revert",
        "db:sync": "npm run typeorm:cli schema:sync && npm run db:migrate",
        "seed": "ts-node src/scripts/seed.ts",
        "format": "prettier --write \"src/**/*.{js,ts,json,css,md}\""
    },
    "author": "Hard team",
    "license": "MIT",
    "dependencies": {
        "@apidevtools/swagger-parser": "^10.1.1",
        "@google-cloud/translate": "^9.0.1",
        "@nestjs/typeorm": "^11.0.0",
        "axios": "^1.8.4",
        "bcryptjs": "^2.4.3",
        "bull": "^4.16.5",
        "chalk": "^4.1.2",
        "class-validator": "^0.14.1",
        "cors": "^2.8.5",
        "cron": "^4.1.0",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^7.5.0",
        "geoip-lite": "^1.4.10",
        "helmet": "^8.0.0",
        "http-status-codes": "^2.3.0",
        "i18n-iso-countries": "^7.14.0",
        "ioredis": "^5.6.0",
        "joi-extract-type": "^15.0.8",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.11.3",
        "nestjs": "^0.0.1",
        "node-cron": "^3.0.3",
        "nodemailer": "^6.10.0",
        "pdfkit": "^0.17.0",
        "qrcode": "^1.5.4",
        "react-hot-toast": "^2.4.1",
        "reflect-metadata": "^0.2.2",
        "remove": "^0.1.5",
        "request-ip": "^3.3.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.20",
        "uuid": "^11.1.0",
        "yamljs": "^0.3.0",
        "zod": "^3.25.28"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/geoip-lite": "^1.4.4",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.7.5",
        "@types/node-cron": "^3.0.11",
        "@types/nodemailer": "^6.4.16",
        "@types/pdfkit": "^0.13.9",
        "@types/qrcode": "^1.5.5",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/yamljs": "^0.2.34",
        "nodemon": "^3.1.7",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3"
    }
}
